{
    "name": "uptech/upshield-waf",
    "description": "Enterprise-Grade Web Application Firewall for WordPress",
    "version": "1.1.0",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "UpTech",
            "email": "info@uptech.vn",
            "homepage": "https://uptech.vn"
        }
    ],
    "homepage": "https://github.com/nhattnh/upshield",
    "support": {
        "issues": "https://github.com/nhattnh/upshield/issues",
        "source": "https://github.com/nhattnh/upshield"
    },
    "keywords": [
        "wordpress",
        "security",
        "firewall",
        "waf",
        "protection",
        "malware",
        "scanner"
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "UpShield\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress .",
        "phpcbf": "phpcbf --standard=WordPress ."
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}